{
  "name": "Jackett",
  "version": "development",
  "slug": "jackett",
  "description": "Jackett add-on based on the prebuilt docker image from linuxserver",
  "url": "https://github.com/tkdrob/hassio-addons/tree/master/jackett",
  "startup": "application",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "9117/tcp": 9117
  },
  "ports_description": {
    "9117/tcp": "Web interface"
  },
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:9117]",
  "ingress": true,
  "ingress_port": 9117,
  "panel_icon": "mdi:download",
  "apparmor": true,
  "init": false,
  "map": ["ssl"],
  "host_network": false,
  "boot": "auto",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {},
  "schema": {}
}
