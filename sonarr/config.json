{
  "name": "Sonarr",
  "version": "175",
  "slug": "sonarr_docker",
  "description": "Sonarr add-on based on the linuxserver/sonarr docker image",
  "url": "https://github.com/marcelveldt/hassio-addons-repo/sonarr",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "ports": {
    "8989/tcp": 8989
  },
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8989]",
  "boot": "auto",
  "options": {
    "log_level": "info"
  },
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "schema": {}
}